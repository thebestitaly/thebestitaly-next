{
  "name": "uuid-migration-scripts",
  "version": "1.0.0",
  "description": "Scripts per migrazione UUID nelle collections Directus",
  "type": "module",
  "scripts": {
    "migrate:uuid": "node add-uuid-migration.js",
    "migrate:uuid:dry-run": "node add-uuid-migration.js --dry-run",
    "migrate:translations": "node migrate-existing-translations.js migrate",
    "check:translations": "node migrate-existing-translations.js check"
  },
  "dependencies": {
    "pg": "^8.11.3",
    "uuid": "^9.0.1",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/pg": "^8.10.9",
    "@types/uuid": "^9.0.7"
  }
} 